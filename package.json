{
  "name": "ace-end-to-end-test",
  "version": "0.0.0",
  "description": "Repository containing All e2e tests",
  "main": "playwright.config.ts",
  "scripts": {
    "initial-setup": "rm -rf node_modules && rm package-lock.json && npm install && npx playwright install chromium",
    "download-chromium": "npx playwright install chromium",
    "download-playwright-chrome": "npx playwright install && npx playwright install chromium",
    "test": "cross-env NODE_NO_WARNINGS=1 npx playwright test",
    "postinstall": "husky install",
    "testcase-registration": "npx playwright test --grep @tds-setup",
    "test-data-creation": "ts-node src/test-data/tds.config.ts",
    "update": "npx npm-check-updates -u --reject axios"
  },
  "author": "Damayanti",
  "license": "Priavte",
  "prettier": "@alphabet/prettier-config",
  "devDependencies": {
    "@alphabet/prettier-config": "0.1.0",
    "@faker-js/faker": "^7.6.0",
    "@playwright/test": "1.29.1",
    "@types/jasmine": "4.3.1",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "allure-playwright": "2.0.0-beta.23",
    "axios": "0.27.2",
    "axios-retry": "3.3.1",
    "cross-env": "7.0.3",
    "eslint": "8.31.0",
    "dotenv": "16.0.3",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-playwright": "0.11.2",
    "eslint-plugin-prettier": "4.2.1",
    "form-data": "4.0.0",
    "glob": "8.0.3",
    "https-proxy-agent": "5.0.1",
    "husky": "8.0.3",
    "jasmine": "4.5.0",
    "jasmine-ts": "0.4.0",
    "jwt-decode": "3.1.2",
    "lint-staged": "13.1.0",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "npm-check-updates": "16.6.2",
    "pg-promise": "11.0.2",
    "prettier": "2.8.1",
    "qs": "6.11.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix --max-warnings=0 --no-ignore",
      "prettier --write"
    ]
  }
}
